program project1;
uses unit1;
var y:TFloatMemStorage;
    x:TIntMemStorage;

begin
  x := TIntMemStorage.Create;
  x[0] := 2;
  x[7] := 5;
  writeln(x[0]);
  writeln(x[7]);
  writeln(x[6]);
  writeln(x.count);
  x.free;

  y := TFloatMemStorage.Create;
  y[1] := 0.999;
  y[4] := 3.14;
  writeln(y[1]:0:2);
  writeln(y[4]:0:2);
  writeln(y.count);
  y.free;
end.

